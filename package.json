{
  "name": "wetube",
  "version": "1.0.0",
  "description": "The best way to watch videos.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ANNA8020/wetube-reloaded.git"
  },
  "scripts": {
    "start": "node build/init.js",
    "build": "npm run build:server && npm run build:assets",
    "build:server": "babel src -d build",
    "build:assets": "webpack --mode=production",
    "dev": "nodemon --exec babel-node src/init.js",
    "dev:assets": "webpack --mode=development -w"
  },
  "author": "Anna",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ANNA8020/wetube-reloaded/issues"
  },
  "homepage": "https://github.com/ANNA8020/wetube-reloaded#readme",
  "dependencies": {
    "express": "^4.17.1",
    "mongoose": "^6.0.9",
    "morgan": "^1.10.0",
    "package.json": "^2.0.1",
    "pug": "^3.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.5",
    "@babel/node": "^7.14.9",
    "@babel/preset-env": "^7.15.6",
    "babel-loader": "^8.2.2",
    "nodemon": "^2.0.12",
    "webpack": "^5.57.1",
    "webpack-cli": "^4.8.0"
  }
}
